{
  "name": "guaira-pos-web",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "next dev -p 9300",
    "build": "next build",
    "start": "next start -p 9300",
    "lint": "next lint",
    "prisma:seed": "tsx prisma/seed.ts",
    "prisma:studio": "prisma studio",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=__tests__/unit",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:e2e": "jest --testPathPattern=__tests__/e2e",
    "test:security": "jest --testPathPattern=__tests__/security",
    "test:performance": "jest --testPathPattern=__tests__/performance --runInBand",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:coverage": "jest --coverage --collectCoverageFrom='app/**/*.{ts,tsx}'",
    "test:verbose": "jest --verbose --coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:clear": "jest --clearCache"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@next-auth/prisma-adapter": "^1.0.7",
